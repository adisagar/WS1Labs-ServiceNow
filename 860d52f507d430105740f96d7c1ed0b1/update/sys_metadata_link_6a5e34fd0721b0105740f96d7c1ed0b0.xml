        &lt;description/&gt;
        &lt;embedded_help_qualifier/&gt;
        &lt;end_point&gt;/sys_state_model_list.do?sysparm_query=tableINproblem,problem_task&lt;/end_point&gt;
        &lt;end_point_open_mode&gt;3&lt;/end_point_open_mode&gt;
        &lt;end_point_type&gt;1&lt;/end_point_type&gt;
        &lt;field/&gt;
        &lt;filter/&gt;
        &lt;filter_result/&gt;
        &lt;implicit_dependent_on/&gt;
