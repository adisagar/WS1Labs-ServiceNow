        <name>ws1_get_table</name>
        <script><![CDATA[var ws1_get_table = Class.create();
ws1_get_table.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	serverCheck: function() {
		
		// check if the CMDB connector is installed
		var ts = new GlideRecord("sys_store_app"); 				
		ts.addQuery("name","Service Graph Connector for VMware Workspace ONE UEM");
		ts.query();
		ts.next();

		if (ts.name) {
			return true;
		}
		
		return false;
	},
    type: 'ws1_get_table'
});]]></script>
